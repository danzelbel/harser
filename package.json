{
	"preview": true,
	"name": "harser",
	"displayName": "harser",
	"description": "Generate code from HAR files",
	"version": "2020.9.1",
	"publisher": "danzelbel",
	"author": {
		"name": "danzelbel"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/danzelbel/harser"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"har",
		"restsharp",
		"extension"
	],
	"license": "MIT",
	"activationEvents": [
		"onView:harser.requestsView"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsWelcome": [
			{
				"view": "harser.requestsView",
				"contents": "[Open a file](command:vscode.window.showOpenDialog) which contains a HAR entry."
			}
		],
		"views": {
			"explorer": [
				{
					"id": "harser.requestsView",
					"name": "Harser"
				}
			]
		},
		"commands": [
			{
				"command": "harser.requestsView.all",
				"title": "All"
			},
			{
				"command": "harser.requestsView.xhr",
				"title": "XHR"
			},
			{
				"command": "harser.requestsView.restsharp.cs",
				"title": "C# - RestSharp"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "harser.requestsView.all",
					"when": "view == harser.requestsView"
				},
				{
					"command": "harser.requestsView.xhr",
					"when": "view == harser.requestsView"
				}
			],
			"view/item/context": [
				{
					"command": "harser.requestsView.restsharp.cs",
					"when": "view == harser.requestsView && viewItem == request"
				}
			]
		},
		"configuration": {
			"title": "Harser",
			"properties": {
				"harser.template.restsharp.cs": {
					"type": "string"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"package": "vsce package -o harser-insider.vsix",
		"deploy": "vsce publish"
	},
	"devDependencies": {
		"@types/debounce": "^1.2.0",
		"@types/fs-extra": "^9.0.1",
		"@types/glob": "^7.1.1",
		"@types/har-format": "^1.2.4",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vsce": "^1.79.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"debounce": "^1.2.0",
		"fs-extra": "^9.0.1",
		"handlebars": "^4.7.6"
	}
}
